include ../_include/_variables
doctype html
//- include ../_include/_watermark

html(dir='ltr', lang=config.SEO.seo_lang, itemscope, itemtype="http://schema.org/WebSite", prefix="og: http://ogp.me/ns#", class="no-js")
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
		meta(name='format-detection', content='telephone=no')
		meta(name='msapplication-tap-highlight', content='no')
		link(href='https://fonts.googleapis.com/css?family=Encode+Sans:100,200,300,400,500,600,700,800,900', rel='stylesheet')
		include ../_include/_css
	body#top-page(class=process.argv.slice(3).indexOf('--production') < 0 ? 'canhcam-develop '+prefix+bodyclass+'-page' : prefix+bodyclass+'-page' || 'default')
		#fb-root
		block header
		main
			block body
		block footer
		a(href="#top-page", id="back-to-top", title="Back to top")
			i.fa.fa-angle-double-up
		include ../_include/_script
		block js

